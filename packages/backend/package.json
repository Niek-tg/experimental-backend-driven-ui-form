{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "description": "Express.js backend API for backoffice with EventBridge integration",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
